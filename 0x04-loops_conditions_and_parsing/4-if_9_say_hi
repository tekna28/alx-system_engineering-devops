#!/usr/bin/env bash
# This script displays "Best School" 10 times using a while loop and prints Hi in the 9th iteration.

counter=1

while [ $counter -le 10 ]
do
    if [ $counter -eq 9 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi

    counter=$((counter + 1))
done
